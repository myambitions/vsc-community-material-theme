      {
        "label": "Community Material Theme Ocean",
        "path": "./themes/Community-Material-Theme-Ocean.json",
        "uiTheme": "vs-dark"
      },
      {
        "label": "Community Material Theme Ocean High Contrast",
        "path": "./themes/Community-Material-Theme-Ocean-High-Contrast.json",
        "uiTheme": "vs-dark"
      },
      {
        "label": "Community Material Theme Lighter",
        "path": "./themes/Community-Material-Theme-Lighter.json",
        "uiTheme": "vs"
