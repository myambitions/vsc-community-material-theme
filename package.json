{
  "name": "vsc-community-material-theme",
  "displayName": "Community Material Theme",
  "description": "The official community maintained Material Theme with 'legacy' color schemes you love!",
  "version": "1.0.0",
  "publisher": "Equinusocio",
  "license": "Apache-2.0",
  "author": "Mattia Astorino [@equinusocio]",
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#11131C",
    "theme": "dark"
  },
  "homepage": "https://material-theme.site",
  "repository": {
    "type": "git",
    "url": "https://github.com/material-theme/vsc-community-material-theme.git"
